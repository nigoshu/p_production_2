<div class="main">
<h1>ログイン</h1>
    <%= form_tag("/users/login") do%>
    <div class="form-content">
      <p>名前</p>
      <input name="name">
      <p>アドレス</p>
      <input name="email">
      <p>パスワード</p>
      <input name="password">
      <input type="submit" value="ログイン">
    </div>
    <% end %>
</div>